---
import Svg from "../svg.astro";

interface Props {
  className?: string;
}

const { className } = Astro.props;
---

<Svg className={`size-4 ${className ?? ""}`}>
  <path
    fill="none"
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M7 7h10v10M7 17L17 7"></path>
</Svg>
