---

import {  LayoutReading } from "@/layout";
import {  getAllBookmarkItems } from "@/lib/raindrop";
import { site } from "@/constants";

const entries = await getAllBookmarkItems();
const entriesPerPage = site.pages.reading.entriesPerPage;
const meta = site.pages.reading;



---
<LayoutReading
  entries={entries}
  entriesPerPage={entriesPerPage}
  meta={meta}
  page={1}
  isIndex
/>
    